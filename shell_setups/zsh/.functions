up () {
    this="$1"
    while [ "$this" -gt 0 ] ; do
	cd ..
	let "this--"
    done
}


add_alias () {
    name="$1"
    command="$2"
    if [ -z "$name" -o -z "$command" ] ; then
	cat << EOF
Usage: add_alias name 'command'
adds alias to .alias list and sources allowing for instant use
EOF
	return 1
    fi
    cat << EOF >> ~/zsh/.alias
alias $name='$command'
EOF
    source ~/zsh/.alias
}

add_cfg () {
    name="$1"
    config="$2"
    if [ -z "$name" -o -z "$config" ] ; then
    cat << EOF
Usage: add_cfg name 'config'
adds config to .cfg list for use in cfg
EOF
    return 1
    fi
    if [ -f "$config" ] ; then
    cat << EOF >> ~/.local/bin/cfg.conf
$name = "$config"
EOF
    else
        echo "Config file $config does not exist" >&2
        return 2
    fi

}
